pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
-- no rula
-- by wojtek

function _init()
 p={}     -- player
 p.s=1    -- player sprite
 p.x=64   -- player position
 p.y=56   -- player position
 p.sx=1  -- player speed
 p.sy=2
 g=0.7
end

function _update()
  handleinput()
end

function _draw()
  cls()
  drawmap()
  dplayer()
  gravity()
end

function gravity()
  if(ccols()!="d") then 
    p.y+=g
  end
end

function handleinput()
  p.s=1
  --left
  if(btn(0))then
    p.x-=p.sx
  end
  --right
  if(btn(1))then
    p.x+=p.sx
    if (p.x > 120) then
      camera(120,0)
    end
  end
  --up
  if(btn(2))then
    jump()
  end
  --down
  if(btn(3) and ccols()!="d") then --down
    p.y+=p.sy
  end
end

function choosespr()
end

-- check collisions
function ccols()
  if (mget((p.x+4)/8, (p.y+8)/8)==16)
    then return "d"
  end 
end

function drawmap()
  map(0,0, 0,0, 50,16)
end

function dplayer()
  spr(p.s, p.x, p.y)
end
-->8
function jump()
  p.s=3
  p.y-=p.sy
end 
